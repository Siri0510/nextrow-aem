<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="nt:unstructured"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab
                jcr:primaryType="cq:Widget"
                title="Basic"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="enter text"
                        name="./title"
                        xtype="textfield"/>
                </items>
            </tab>
            <tab1
                jcr:primaryType="cq:Widget"
                title="content"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <thumbnail
                        jcr:primaryType="cq:Widget"
                        fieldLabel="enter value"
                        name="./title1"
                        xtype="textfield"/>
                    <path
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Path"
                        name="./path"
                        xtype="pathfield"/>
                    <numberfield
                        jcr:primaryType="cq:Widget"
                        fieldLabel="enter number"
                        name="./num"
                        xtype="numberfield"/>
                    <textarea
                        jcr:primaryType="cq:Widget"
                        fieldLabel="text"
                        name="./area"
                        xtype="textarea"/>
                    <checkbox
                        jcr:primaryType="cq:Widget"
                        fieldLabel="male"
                        name="./select"
                        xtype="checkbox"/>
                    <checkbox1
                        jcr:primaryType="cq:Widget"
                        fieldLabel="female"
                        name="./select1"
                        xtype="checkbox"/>
                    <image1
                        jcr:primaryType="cq:Widget"
                        sling:resourceType="foundation/components/image"
                        ddGroups="media"
                        fieldLabel="image1"
                        fileNameParameter="./image1/fileName"
                        height="{Long}200"
                        name="./image1"
                        title="Image1"
                        width="{Long}200"
                        xtype="html5smartimage"/>
                </items>
            </tab1>
            <images
                jcr:primaryType="cq:Panel"
                title="images">
                <items jcr:primaryType="cq:WidgetCollection">
                    <image1
                        jcr:primaryType="cq:Widget"
                        sling:resourceType="&#x9;foundation/components/image"
                        ddGroups="media"
                        fieldLabel="image1"
                        fileNameParameter="./image1/fileName"
                        height="{Long}200"
                        name="./image1"
                        title="Image1"
                        width="{Long}200"
                        xtype="html5smartimage"/>
                </items>
            </images>
        </items>
    </items>
</jcr:root>
